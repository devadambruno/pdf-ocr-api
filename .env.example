GCP_PROJECT_ID=
GCP_LOCATION=us
DOCUMENT_AI_PROCESSOR_ID=
# Se true, tenta processar PDFs com texto nativo (sem OCR de imagens).
# Em PDFs escaneados, geralmente deve ficar false (ou vazio) para permitir OCR e detecção de tabelas.
DOCUMENT_AI_IMAGELESS_MODE=false

# Uma destas duas formas de autenticação do Google deve estar configurada:
# 1) JSON do service account em uma variável:
GOOGLE_APPLICATION_CREDENTIALS_JSON=
# 2) Ou um arquivo local:
GOOGLE_APPLICATION_CREDENTIALS=./google-sa.json

OPENAI_API_KEY=
XANO_BASE_URL=
